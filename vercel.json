{
    "version": 2,
    "builds": [
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      {
        "src": "/analyze",
        "methods": ["POST"],
        "dest": "/api/analyze"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "STATSGARY_MODEL_NAME": "StatsGary/setfit-ft-sentinent-eval"
    }
  }
  